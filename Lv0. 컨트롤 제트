class Solution {
    public int solution(String s) {
        int answer = 0;
        
        String[] hello = new String[200];
    
        hello=s.split(" ");
        try{
            for(int i=0; i<200; i++){
                if(hello[i]==null) break;
                else if(hello[i]=="Z") answer=answer-Integer.parseInt(hello[i-1]);
                else answer=answer+Integer.parseInt(hello[i]);
            }
        }catch(NumberFormatException e){
            e.printStackTrace();
        }
            
        return answer;
    }
}
